  "dependencies": {
    "gatsby": "^4.2.0",
    "gatsby-plugin-sitemap": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
